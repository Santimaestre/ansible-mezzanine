[group:mezzanine]
programs=gunicorn_mezzanine

[program:gunicorn_mezzanine]
command={{ venv_bin_dir }}/gunicorn_django -c gunicorn.conf.py -p gunicorn.pid
directory={{ project_dir }}
user={{ ansible_user_id }}
autostart=true
autorestart=true
redirect_stderr=true
environment=LANG="{{ locale }}",LC_ALL="{{ locale }}",LC_LANG="{{ locale }}"
